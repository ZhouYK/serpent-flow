{
  "name": "serpent-flow",
  "version": "1.0.7",
  "description": "react form solver",
  "main": "./cjs/index.js",
  "module": "./es/index.js",
  "unpkg": "./dist/index.js",
  "types": "./es/index.d.ts",
  "scripts": {
    "umd": "cross-env NODE_ENV=production webpack --env prod --config  ./webpack/webpack.config.babel.js",
    "tsc:es": "rm -rf ./es && tsc --project tsconfig/tsconfig.es.json",
    "tsc:cjs": "rm -rf ./cjs && tsc --project tsconfig/tsconfig.cjs.json",
    "build": "npm run umd && npm run tsc:es && npm run tsc:cjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZhouYK/serpent.git"
  },
  "keywords": [
    "react",
    "form"
  ],
  "author": "zhouyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZhouYK/serpent/issues"
  },
  "homepage": "https://github.com/ZhouYK/serpent#readme",
  "dependencies": {
    "classnames": "^2.5.1"
  },
  "peerDependencies": {
    "femo": ">=3.3.0",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@arco-plugins/webpack-react": "^1.4.9",
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-runtime": "7.13.10",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-react": "7.16.0",
    "@babel/register": "7.11.5",
    "@babel/runtime-corejs3": "7.13.10",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.15",
    "@types/classnames": "2.2.10",
    "@types/jest": "^27.5.2",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "5.1.5",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "babel-loader": "8.4.1",
    "clean-webpack-plugin": "3.0.0",
    "core-js": "3.17.2",
    "cross-env": "^7.0.3",
    "css-loader": "3.5.3",
    "eslint": "7.24.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "23.10.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-webpack-plugin": "2.5.4",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "html-webpack-plugin": "5.2.0",
    "husky": "6.0.0",
    "jest": "27.2.4",
    "less": "4.1.2",
    "less-loader": "10.0.1",
    "lint-staged": "11.2.0",
    "mini-css-extract-plugin": "2.4.2",
    "react-refresh": "0.10.0",
    "react-refresh-typescript": "2.0.0",
    "style-loader": "3.3.0",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "ts-jest": "27.0.5",
    "ts-loader": "9.5.2",
    "typescript": "4.8.3",
    "webpack": "5.94.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.3.1"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "verbose": true,
    "transform": {
      "^.+\\.(ts|js)x?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.json"
      }
    }
  },
  "files": [
    "es",
    "cjs",
    "dist",
    "package.json"
  ]
}
